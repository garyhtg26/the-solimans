  "main": "index.js",
  "scripts": {
    "dev:css": "npx @tailwindcss/cli -i ./assets/css/tailwind.css -o ./assets/css/main.css --watch --content './**/*.{html,liquid,md,markdown}' --content './_data/**/*.{yml,yaml,json}'",
    "serve": "bundle exec jekyll serve",
    "start": "npm run dev:css & npm run serve",
    "build:css": "npx @tailwindcss/cli -i ./assets/css/tailwind.css -o ./assets/css/main.css --minify --content './**/*.{html,liquid,md,markdown}' --content './_data/**/*.{yml,yaml,json}'",
    "build": "bundle exec jekyll build && npm run build:css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
